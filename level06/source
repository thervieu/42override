int auth(char *username, char *serialized) {
    if strlen(username) < 5
      return 0;
    ptrace(0)
    // serialize 'username' into 'actual_serialized'
    return (serialized == actual_serialized);
}

int main() {
    puts("***********************************\n"
        "*		level06		  *\n"
        "***********************************\n");
    printf("-> Enter Login: ");
    fgets(username);

    puts("***********************************\n"
          "***** NEW ACCOUNT DETECTED ********\n"
          "***********************************\n");
    printf("-> Enter Serial: ");
    fgets(serialized);

    if (strlen(username) > 5) {
